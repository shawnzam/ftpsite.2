<%= simple_form_for(@project) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :address_1 %>
    <%= f.input :address_2 %>
    <%= f.input :due_date %>
    <%= f.input :notes %>
    <%= f.input :zipcode %>
    <%= f.association :user %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
